geTab() {\r\n            this.listTab = this.listTab === \"new\" ? \"hot\" : \"new\";\r\n        },\r\n\r\n        doExercise(id) {\r\n            this.$router.push(\"/exercise/detail/\" + id);\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n.exercise {\r\n    padding: 40px;\r\n\r\n    .listTab {\r\n        margin: 34px 0 10px;\r\n\r\n        span {\r\n            font-size: 18px;\r\n            margin-right: 12px;\r\n            padding: 8px 20px;\r\n            border-radius: 8px;\r\n            cursor: pointer;\r\n        }\r\n\r\n        .active {\r\n            background-color: #ff0000;\r\n            color: #FFFFFF;\r\n        }\r\n    }\r\n\r\n    .content {\r\n        width: 100%;\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n\r\n        .item {\r\n            width: 17.5%;\r\n            margin: 24px 24px 0 0;\r\n            border-radius: 15px;\r\n            box-shadow: 0px 0px 10px rgba(7, 17, 27, 0.1);\r\n            padding: 24px 32px 32px;\r\n            cursor: pointer;\r\n\r\n            .mark {\r\n                font-size: 12px;\r\n                line-height: 16px;\r\n                margin-bottom: 8px;\r\n                vertical-align: middle;\r\n\r\n                .icon, .text {\r\n                    vertical-align: middle;\r\n                }\r\n\r\n                .icon {\r\n                    margin-right: 4px;\r\n                    color: #FF0000;\r\n                }\r\n            }\r\n\r\n            .title {\r\n                display: inline-block;\r\n                width: 100%;\r\n                font-size: 16px;\r\n                color: #07111B;\r\n                line-height: 24px;\r\n                cursor: pointer;\r\n                transition: all 0.3s;\r\n                margin-bottom: 5px;\r\n                word-wrap: break-word;\r\n                overflow: hidden;\r\n                text-overflow: ellipsis;\r\n                display: -webkit-box;\r\n                -webkit-box-orient: vertical;\r\n                -webkit-line-clamp: 2;\r\n            }\r\n\r\n            &:hover {\r\n                .title {\r\n                    color: #FF0000;\r\n                    opacity: 0.8;\r\n                }\r\n\r\n                .footer {\r\n                    display: none;\r\n                }\r\n\r\n                .do {\r\n                    display: block;\r\n                    color: #fff;\r\n                    border-radius: 4px;\r\n                    padding: 10px 0;\r\n                    background: #FF0000D0;\r\n                }\r\n\r\n                & {\r\n                    padding-bottom: 12px;\r\n                }\r\n            }\r\n\r\n            .pic {\r\n                width: 100%;\r\n                height: 120px;\r\n                background-size: 100% 100%;\r\n                border-radius: 12px;\r\n                margin: 10px 0;\r\n            }\r\n\r\n            .footer {\r\n                font-size: 12px;\r\n                color: #93999f;\r\n                line-height: 24px;\r\n                font-weight: 400;\r\n\r\n                &:after {\r\n                    content: '';\r\n                    display: block;\r\n                    clear: both;\r\n                }\r\n\r\n                .diffculty {\r\n                    display: inline-block;\r\n                }\r\n\r\n                .number {\r\n                    display: inline-block;\r\n                    margin-left: 10px;\r\n\r\n                    .iconfont {\r\n                        font-size: 12px;\r\n                    }\r\n                }\r\n\r\n                .grade {\r\n                    display: inline-block;\r\n                    margin: 0 -8px;\r\n                    transform: translateY(-2px) scale(0.7);\r\n                }\r\n            }\r\n\r\n            .do {\r\n                display: none;\r\n                text-align: center;\r\n            }\r\n        }\r\n\r\n        .item:nth-child(4n +